     <tr> <td> <code title="">Ccirc;</code> </td> <td> U+00108 </td> </tr>
     <tr> <td> <code title="">Cconint;</code> </td> <td> U+02230 </td> </tr>
     <tr> <td> <code title="">Cdot;</code> </td> <td> U+0010A </td> </tr>
