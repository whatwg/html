     <tr> <td> <code title="">CenterDot;</code> </td> <td> U+000B7 </td> </tr>
     <tr> <td> <code title="">Cfr;</code> </td> <td> U+0212D </td> </tr>
     <tr> <td> <code title="">Chi;</code> </td> <td> U+003A7 </td> </tr>
